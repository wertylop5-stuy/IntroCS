(display "#0 max3, revisited") (newline)

(define max3
  (lambda (a b c)
    (if (> a b)
        (if (> a c)
            a
            (if (< a c)
                c
                a);else, they equal
            )
        (if (> b a)
            (if (> b c)
                b
                (if (< b c)
                    c
                    b);else, they equal
                )
            (if (> c b);a and b are equal
                c
                a)
            )
        )))

;All of these should return 3
(display (max3 1 2 3))(newline)
